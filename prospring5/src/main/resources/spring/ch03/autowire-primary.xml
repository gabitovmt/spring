<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation=
               "http://www.springframework.org/schema/beans
                http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="fooOne" class="spring.ch03.autowiretype.FooImplOne"
          primary="true"/>
    <bean id="fooTwo" class="spring.ch03.autowiretype.FooImplTwo"/>
    <bean id="barOne" class="spring.ch03.autowiretype.Bar"/>

    <bean id="targetByType" autowire="byType"
          class="spring.ch03.autowireprimary.CTarget"
          lazy-init="true"/>
</beans>